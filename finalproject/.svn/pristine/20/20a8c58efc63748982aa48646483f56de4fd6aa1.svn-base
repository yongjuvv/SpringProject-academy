<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config
3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="movie" 		type="com.ch.project.model.Movie" />
		<typeAlias alias="pmember" 		type="com.ch.project.model.PMember" />
		<typeAlias alias="tv" 			type="com.ch.project.model.Tv" />
		<typeAlias alias="review" 		type="com.ch.project.model.Review" />
		<typeAlias alias="hashtag" 		type="com.ch.project.model.Hashtag" />
		<typeAlias alias="pcharge" 		type="com.ch.project.model.PCharge" />
